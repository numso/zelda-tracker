{
  "name": "zelda-tracker",
  "version": "0.0.1",
  "scripts": {
    "build:cname": "echo 'zelda.bydall.in' > dist/CNAME",
    "build:copy-files": "cp client/index.html dist/",
    "build:js": "webpack --config webpack-production.config.js",
    "build": "npm-run-all clean --parallel build:*",
    "clean": "rm -rf dist/*",
    "deploy": "cd dist && git add -A . && git commit -m 'deploying' && git push origin gh-pages",
    "start": "webpack-dev-server --hot --inline --port 5001"
  },
  "dependencies": {
    "autoprefixer": "6.3.6",
    "babel-core": "6.8.0",
    "babel-eslint": "6.0.4",
    "babel-loader": "6.2.4",
    "babel-plugin-react-transform": "2.0.2",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-1": "6.5.0",
    "css-loader": "0.23.1",
    "eslint": "2.10.1",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-filenames": "0.2.0",
    "eslint-plugin-import": "1.8.0",
    "eslint-plugin-jsx-a11y": "1.2.0",
    "eslint-plugin-lodash": "1.8.4",
    "eslint-plugin-no-useless-assign": "1.0.2",
    "eslint-plugin-promise": "1.1.0",
    "eslint-plugin-react": "5.1.1",
    "firebase": "2.4.2",
    "json-loader": "0.5.4",
    "lodash": "4.12.0",
    "npm-run-all": "2.1.0",
    "postcss-loader": "0.9.1",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4",
    "redbox-react": "1.2.4",
    "style-loader": "0.13.1",
    "webpack": "1.13.0",
    "webpack-dev-server": "1.14.1"
  }
}
